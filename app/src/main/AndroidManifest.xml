<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.fengfancky.jetpackdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".behavior.ShareActionActivity"/>
        <activity android:name=".Animation.FlingAnimationActivity"/>
        <activity android:name=".Animation.SpringAnimationActivity"/>
        <activity android:name=".Animation.TranslationActivity"/>
        <activity android:name=".Animation.ZoomActivity"/>
        <activity android:name=".Animation.LayoutUpdateActivity"/>
        <activity android:name=".behavior.NotificationActivity"/>
        <activity android:name=".behavior.ResultActivity"/>
        <activity android:name=".Animation.PaletteActivity"/>
        <activity android:name=".navigation.NavigationActivity"
            android:theme="@style/FragmentTranslation"/>

    </application>

</manifest>